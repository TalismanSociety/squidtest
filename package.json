{
  "name": "kusama-balances",
  "private": true,
  "scripts": {
    "build": "rm -rf lib && tsc",
    "init": "node db/init.js",
    "db:migrate": "sqd db migrate",
    "schema:reset": "",
    "processor:migrate": "echo skip",
    "processor:start": "node lib/processor.js",
    "query-node:start": "squid-graphql-server",
    "testout": "npm run build && node -r dotenv/config lib/processor.js",
    "migrationreset": "rm -rf db/migrations/*.js && npx sqd db drop && npx sqd db create &&npx sqd db create-migration Init && npx sqd db migrate"
  },
  "dependencies": {
    "@polkadot/api": "^8.0.2",
    "@polkadot/keyring": "^9.4.1",
    "@polkadot/types": "^8.0.2",
    "@subsquid/archive-registry": "^0.0.6",
    "@subsquid/graphql-server": "^0.1.5",
    "@subsquid/ss58": "^0.1.0",
    "@subsquid/substrate-processor": "^0.7.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.0",
    "graphql-request": "^4.3.0",
    "md5": "^2.3.0",
    "pg": "^8.7.3",
    "type-graphql": "^1.2.0-rc.1",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@subsquid/cli": "^0.2.3",
    "@subsquid/substrate-metadata-explorer": "^0.0.7",
    "@subsquid/substrate-typegen": "^0.4.1",
    "@types/node": "^17.0.24",
    "typescript": "~4.5.5"
  }
